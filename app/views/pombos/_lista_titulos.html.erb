<table class="table table-hover" id="tabtitulo" >
  <thead>
    <tr>
      <th><%= t(:label_premio) %></th>
      <th><%= t(:label_ano) %></th>
     </tr>
  </thead>
      
  <tbody>
    <% @pombo_modal.titulos.order('ano DESC').each do |titulo| %>
      <tr>
        <td>
          <% if logged_in? && !visivel_correct_user(@pombo_modal.id.to_s) %>
            <%= link_to destroy_titulo_pombos_path(id: titulo.id), method: :delete, data: { confirm: t(:msg_quest_apagar_padrao) }, class: botao_link, remote: true, :title => t(:label_remover), :rel => 'tooltip', "data-placement" => "bottom" do %>
              <span class="fa fa-trash"></span>
            <% end %>
          <% end %>
          <%= titulo.nome %>
        </td>
        <td><%= titulo.ano %></td>
      </tr>
    <% end %>
  </tbody>
</table>