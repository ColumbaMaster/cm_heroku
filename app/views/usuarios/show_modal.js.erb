<% if @modal == MODAL_USU_PAGTO %>
  $("#modal").html("<%= j render 'modal' %>");
  $('#modal_usuario').modal('show');
<% elsif @modal == MODAL_USU_COORD %>
  
  if (document.getElementById('usuario_lat').value != '' && document.getElementById('usuario_lng').value != '') {
    $("#modal").html("<%= j render "shared/modal_mapa"%>");
    $('#modal_mapa').modal('show');
    
    initMapLL(document.getElementById('usuario_lat').value
          , document.getElementById('usuario_lng').value
          , 18); //zoom_tela
          
    preencheValoresCoordenadas('usuario', 'lat');
    preencheValoresCoordenadas('usuario', 'lng');
  }
  else {
  
    if (!valida_coodenadas_diretor('usuario')){
      alert("<%= t :msg_erro_informar_endereco_usuario %>");
    } 
    else {
  
      $("#modal").html("<%= j render "shared/modal_mapa"%>");
      $('#modal_mapa').modal('show');
      
      converterCoordParaDecimal('usuario', 10);
    }
  }
<% end %>